    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.11",
    "cypress": "^10.0.2",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "https-localhost": "^4.7.1",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
