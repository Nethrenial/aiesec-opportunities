    "firebase": "^9.8.2",
    "mosha-vue-toastify": "^1.0.23",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "vite-plugin-webfont-dl": "^2.1.0",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15",
    "vue-virtual-scroll-grid": "^1.7.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.56",
    "@rushstack/eslint-patch": "^1.1.0",
